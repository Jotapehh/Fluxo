{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/models/*.php": {
      "runtime": "vercel-php@0.7.4"
    },
    "api/registers/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/api/index.php" },
    { "source": "/register", "destination": "/api/registers/register.php" },
    { "source": "/models/:slug", "destination": "/api/models/:slug.php" }
  ]
}
